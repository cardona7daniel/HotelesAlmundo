{
  "name": "almundo",
  "version": "1.0.0",
  "description": "Hoteles AlMundo",
  "main": "src",
  "scripts": {
    "start": "nodemon src/app.js --exec babel-node",
    "build": "babel src -d dist",
    "serve": "node dist/app.js"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "keywords": [
    "nodeJs",
    "Express",
    "ES6",
    "ES7"
  ],
  "author": "Luis D. Cardona",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "^4.19.1",
    "nodemon": "^1.17.3"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "compression": "^1.7.2",
    "cors": "^2.8.4",
    "express": "^4.16.3"
  }
}
